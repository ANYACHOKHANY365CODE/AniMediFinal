{
  "expo": {
    "name": "AniMedi - Pet Healthcare",
    "slug": "animedi-pet-healthcare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "animedi",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      "expo-notifications",
      "expo-document-picker",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}